{
  "name": "obsidian-encrypted-folders",
  "version": "1.1.0",
  "description": "Obsidian plugin to encrypt folders within your vault.",
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "encryption",
    "folders"
  ],
  "license": "MIT",
  "author": "Eric N. Garcia <eng618@garciaericn.com>",
  "main": "main.js",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "dev": "node esbuild.config.mjs",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:report": "eslint . --cache -o ./eslintReport.html -f html",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "lint-staged": {
    "*.(js|ts|mjs|cjs)?(x)": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.(md)?(x)": [
      "prettier --write"
    ]
  },
  "prettier": "@eng618/prettier-config",
  "devDependencies": {
    "@eng618/prettier-config": "^2.6.0",
    "@gv-tech/eslint-config": "^0.1.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.11",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "builtin-modules": "5.0.0",
    "esbuild": "0.25.11",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "obsidian": "^1.10.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "tslib": "2.8.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
