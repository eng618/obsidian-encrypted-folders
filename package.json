{
  "name": "obsidian-encrypted-folders",
  "version": "1.0.0",
  "private": true,
  "description": "Obsidian plugin to encrypt folders within your vault.",
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "encryption",
    "folders"
  ],
  "license": "MIT",
  "author": "Eric N. Garcia <eng618@garciaericn.com>",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "lint:report": "eslint . --cache -o ./eslintReport.html -f html",
    "prepare": "husky",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "validate": "./scripts/validate.sh",
    "version": "node scripts/version-bump.mjs && git add manifest.json versions.json"
  },
  "lint-staged": {
    "*.(js|ts|mjs|cjs)?(x)": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.(md)?(x)": [
      "prettier --write"
    ]
  },
  "prettier": "@eng618/prettier-config",
  "devDependencies": {
    "@eng618/prettier-config": "^2.6.0",
    "@gv-tech/eslint-config": "^0.1.8",
    "@types/node": "^22.18.11",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "builtin-modules": "5.0.0",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "obsidian": "^1.10.0",
    "prettier": "^3.8.1",
    "tslib": "2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=22.0.0"
  }
}
